{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Custom Name",
                              "component": "CustomName",
                              "name": "CustomNameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "CustomName",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${car.CustomName}"
                            },
                            {
                              "type": "required-validator",
                              "name": "CustomNameRequiredValidator",
                              "component": "CustomName",
                              "text": "CustomName is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "Name",
                              "name": "NameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Name",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${car.Name}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NameRequiredValidator",
                              "component": "Name",
                              "text": "Name is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Image",
                              "component": "Image",
                              "name": "ImageLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Image",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${car.Image}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ImageRequiredValidator",
                              "component": "Image",
                              "text": "Image is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Production",
                              "component": "Production",
                              "name": "ProductionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "type": "datepicker",
                              "name": "Production",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${car.Production}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ProductionRequiredValidator",
                              "component": "Production",
                              "text": "Production is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Vin",
                              "component": "Vin",
                              "name": "VinLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Vin",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${car.Vin}"
                            },
                            {
                              "type": "required-validator",
                              "name": "VinRequiredValidator",
                              "component": "Vin",
                              "text": "Vin is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Mileage",
                              "component": "Mileage",
                              "name": "MileageLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "double",
                              "type": "numeric",
                              "name": "Mileage",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${car.Mileage}"
                            },
                            {
                              "type": "required-validator",
                              "name": "MileageRequiredValidator",
                              "component": "Mileage",
                              "text": "Mileage is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "User Id",
                              "component": "UserId",
                              "name": "UserIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "UserId",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${car.UserId}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Car",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "SimpleCarManagmentDB.updateCar",
                        "parameters": [
                          {
                            "name": "Id",
                            "value": "${parameters.Id}"
                          },
                          {
                            "name": "Car",
                            "value": "${car}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${car}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "SimpleCarManagment.Models.SimpleCarManagmentDb.Car",
                  "data": "${car}",
                  "visible": "${car != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "SimpleCarManagmentDB.getCarById",
        "parameters": [
          {
            "name": "Id",
            "value": "${parameters.Id}"
          }
        ],
        "then": [
          {
            "name": "car",
            "type": "set",
            "value": "${result}",
            "valueType": "SimpleCarManagment.Models.SimpleCarManagmentDb.Car"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Cars",
  "access": "Authenticated",
  "layout": "Main"
}